{
  "name": "@oneclickdz/ocpay-sdk",
  "version": "1.0.0",
  "description": "Official Node.js SDK for OneClickDz OCPay payment gateway",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "prepublishOnly": "npm run build",
    "prepare": "npm run build"
  },
  "keywords": [
    "oneclickdz",
    "ocpay",
    "payment",
    "gateway",
    "algeria",
    "dzd",
    "typescript",
    "sdk"
  ],
  "author": "oneclickdz <support@oneclickdz.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/oneclickdz/ocpay-node-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/oneclickdz/ocpay-node-sdk/issues"
  },
  "homepage": "https://docs.oneclickdz.com",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.2.0"
  }
}
